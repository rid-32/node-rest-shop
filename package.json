{
  "name": "rest-shop",
  "version": "1.0.0",
  "description": "RESTful API on node.js",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon --exec babel-node -- src/server.js",
    "start": "export NODE_ENV=production && babel-node src/server.js",
    "test": "export NODE_ENV=test && jest --colors",
    "test:watch": "export NODE_ENV=test && jest --colors --watchAll",
    "test:coverage": "export NODE_ENV=test && jest --colors --coverage"
  },
  "keywords": [
    "REST",
    "API",
    "node.js",
    "express"
  ],
  "author": "rid-32",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.1.0",
    "nodemon": "^1.17.5",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0"
  }
}
